      <script type="text/javascript" src="http://opensocial.ark-web.jp/osmvc/sample/client/javascripts/config/environment.js"></script>
      <script type="text/javascript" src="http://opensocial.ark-web.jp/osmvc/sample/client/javascripts/controllers/home_controller.js"></script>
      <script type="text/javascript" src="http://opensocial.ark-web.jp/osmvc/sample/client/javascripts/controllers/pets_controller.js"></script>
      <script type="text/javascript" src="http://opensocial.ark-web.jp/osmvc/sample/client/javascripts/models/dog.js"></script>
      <script type="text/javascript" src="http://opensocial.ark-web.jp/osmvc/sample/client/javascripts/models/user.js"></script>

      <div id='contents'></div>
