      <script type="text/javascript" src="http://opensocial.ark-web.jp/osmvc/sample/client/javascripts/lib/osmvc/action_controller.js"></script>
      <script type="text/javascript" src="http://opensocial.ark-web.jp/osmvc/sample/client/javascripts/lib/osmvc/active_record.js"></script>
